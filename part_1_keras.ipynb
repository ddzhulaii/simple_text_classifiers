   "source": [
    "voc_size = 20000\n",
    "tokenizer = Tokenizer(num_words=voc_size)\n",
    "tokenizer.fit_on_texts(df['text'])\n",
    "\n",
    "seqs = tokenizer.texts_to_sequences(df['text'])\n",
    "data = pad_sequences(seqs, maxlen=50)"
